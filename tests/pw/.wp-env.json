{
    "env": {
        "tests": {
            "port": 9999,
            "core": null,
            "phpVersion": "7.4",
            "plugins": ["WP-API/Basic-Auth", "https://downloads.wordpress.org/plugin/woocommerce.latest-stable.zip", "../../"],
            "themes": ["https://downloads.wordpress.org/theme/storefront.latest-stable.zip"],
            "config": {
                "WP_DEBUG": "true",
                "SCRIPT_DEBUG": "true",
                "WP_DEBUG_LOG": "true",
                "WP_DEBUG_DISPLAY": "true"
            },
            "mappings": {
                ".htaccess": ".htaccess",
                "wp-content/debug.log": "./debuglog/debug.log",
                "db/db.sql": "./db/db.sql"
            }
        }
    }
}
