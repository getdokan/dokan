{
  "name": "dokan_e2e_playwright",
  "version": "1.0.0",
  "description": "dokan e2e test using playwright",
  "main": "index.js",
  "scripts": {
    "nip": "npm install --production",
    "nid": "npm install --include=dev",
    "test": "npx playwright test",
    "test:e2e": "playwright test --project=e2e_tests",
    "test:e2e-setup": "playwright install chromium",
    "test:e2e-i": "playwright test --project=e2e_tests --headed",
    "test:debug": "npx playwright test --debug",
    "test:codegen": "npx playwright codegen",
    "container_path": "wp-env install-path",
    "start:env": "wp-env start",
    "update:env": "wp-env start -- --update",
    "stop:env": "wp-env stop",
    "restart:env": "wp-env stop && wp-env start",
    "reset:db": "wp-env clean all && wp-env start",
    "reset:env": "wp-env destroy all && wp-env start",
    "user-list": "wp-env run cli wp user list",
    "activate:theme": "wp-env run tests-cli 'wp theme activate storefront'",
    "wp-env": "wp-env",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/create-config": "^0.4.3",
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-playwright": "^0.12.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.8",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.32.3",
    "@wordpress/env": "^4.9.0",
    "dotenv": "^16.0.3"
  }
}