    "zip": "node bin/zip",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "release": "npm install && npm run build && npm run clean-files && npm run makepot && npm run version && npm run zip",
